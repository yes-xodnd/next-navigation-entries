{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "declaration": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true /* Ensure that casing is correct in imports. */,
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true
  },
  "include": ["./src"]
}
